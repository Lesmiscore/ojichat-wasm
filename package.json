    "@babel/core": "7.12.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "eslint": "7.11.0",
    "file-loader": "6.1.1",
